  build:
    runs-on: windows-latest

    steps:
    - uses: actions/checkout@v2
    
    - name: Deploy ðŸš€
      env:
        GITHUB_TOKEN: ${{ secrets.GITHUB_TOKEN }}
      run: |
        echo ${{ secrets.GITHUB_TOKEN }}
        echo $GITHUB_TOKEN
        git clone https://janiak-minova:$GITHUB_TOKEN@github.com/minova-afis/aero.minova.rcp.updatesite # This works
