<?xml version="1.0" encoding="UTF-8"?>
<main xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="XMLMenu.xsd"
 icon="MINOVA.ico"
 title="Aircraft Fuelling Information System">
    <!-- Stammdaten -->
    <action action="ch.minova.masterdata.TaxResponsableForm" icon="TaxResponsable.ico" id="TaxResponsable" text="@TaxResponsable" shortcut="CTRL_SHIFT_V"/>
    <action action="ch.minova.masterdata.CustomerForm" icon="Customer.ico" id="Customer" text="@Customer" shortcut="CTRL_SHIFT_C"/>
    <action action="ch.minova.masterdata.SupplierForm" icon="Supplier.ico" id="Supplier" text="@Supplier" shortcut="CTRL_SHIFT_E"/>
    <action action="ch.minova.masterdata.TruckForm" icon="Truck.ico" id="Truck" text="@Truck" shortcut="CTRL_SHIFT_W"/>
    <action action="ch.minova.masterdata.tank.TankForm" icon="Tank.ico" id="Tank" text="@Tank" shortcut="CTRL_SHIFT_T"/>
    <action action="ch.minova.masterdata.ItemForm" icon="Item.ico" id="Item" text="@Item" shortcut="CTRL_SHIFT_I"/>
    <action action="ch.minova.masterdata.CompulsaryItemForm" icon="CompulsaryItem.ico" id="CompulsaryItem" text="@CompulsaryItem" shortcut="CTRL_SHIFT_P"/>
    <action action="ch.minova.masterdata.DriverForm" icon="Driver.ico" id="Driver" text="@Driver"  shortcut="CTRL_SHIFT_D"/>
    <action action="ch.minova.masterdata.AircraftForm" icon="Airplane.ico" id="Aircraft" text="@Aircraft" shortcut="CTRL_SHIFT_A"/>
    <action action="ch.minova.masterdata.TaxCodeForm" icon="Tax.ico" id="TaxCode" text="@TaxCode" shortcut="CTRL_SHIFT_S"/>
    <!-- Abwicklung -->
    <action action="ch.minova.afis.booking.ShipmentForm" icon="Shipment.ico" id="Shipment" text="@Shipment" shortcut="CTRL_S"/>
    <action action="ch.minova.afis.dispatch.DispatchForm" dialog="true" icon="Dispatch.ico" id="Dispatch" text="@Dispatch" shortcut="CTRL_D"/>
    <action action="ch.minova.afis.booking.PreDeliveriesForm" icon="FuellingInput.ico" id="PreDeliveries" text="@PreDeliveries"/>
    <action action="ch.minova.afis.booking.RBFuellingForm" icon="RBFuelling.ico" id="RBFuelling" text="@RBFuelling"/>
    <action action="ch.minova.afis.booking.ShipmentForm" icon="ShipmentAircraft.ico" id="Shipment" text="@Shipment"/>
    <action action="ch.minova.afis.booking.GAFuellingForm" icon="FuellingInputGA.ico" id="GAFuelling" text="@GAFuelling"/>
    <action action="ch.minova.afis.CopyFlightScheduleForm" id="CopyFlightSchedule" text="@CopyFlightSchedule" shortcut="CTRL_C"/>
    <!-- Tanklager -->
    <action action="ch.minova.tta.shortbooking.ShortEntryForm" id="ShortEntry" text="@ShortEntry" shortcut="CTRL_E"/>
    <action action="ch.minova.tta.shortbooking.ShortStockExitForm" id="ShortStockExit" text="@ShortStockExit" shortcut="CTRL_X"/>
    <action action="ch.minova.tta.booking.InternalForm" id="Internal" text="@Internal" shortcut="CTRL_I"/>
    <action action="ch.minova.masterdata.tank.TankMeasurementForm" id="TankMeasurement" text="@TankMeasurement"/>
    <action action="ch.minova.tta.StockForm" icon="Stock.ico" id="Stock" text="@Stock"/>
    <action action="ch.minova.tta.STIMForm" icon="STIM.ico" id="STIM" text="@STIM"/>
    <!-- Konfiguration -->
    <action action="ch.minova.afis.dataexchange.FlightFilterExchangeForm" id="FlightFilterExchange" text="@FlightFilterExchange"/>
    <action action="ch.minova.masterdata.FlightFilterForm" icon="FlightFilter.ico" id="FlightFilter" text="@FlightFilter"/>
    <action action="ch.minova.masterdata.FlightFieldPositionForm" id="FlightFieldPosition" text="@FlightFieldPosition"/>
    <action action="ch.minova.masterdata.AircraftTypeForm" id="AircraftType" text="@AircraftType"/>
    <action action="ch.minova.masterdata.AirportForm" icon="Airport.ico" id="Airport" text="@Airport"/>
    <action action="ch.minova.masterdata.CreditCardCompanyForm" id="CreditCardCompany" text="@CreditCardCompany"/>
    <action action="ch.minova.masterdata.VatForm" icon="Vat.ico" id="Vat" text="@Vat"/>
    <action action="ch.minova.masterdata.CompulsaryNameForm" icon="CompulsaryName.ico" id="CompulsaryName" text="@CompulsaryName"/>
    <action action="ch.minova.masterdata.CompulsaryStockDetailForm" icon="CompulsaryStockDetail.ico"
        id="CompulsaryStockDetail" text="@CompulsaryStockDetail"/>
    <action action="ch.minova.afis.TruckPoolForm" icon="TruckPool.ico" id="TruckPool" text="@TruckPool"/>
    <!-- Zuordnung -->
    <action action="ch.minova.masterdata.LuCustomerFlightNumberForm" id="LuCustomerFlightNumber" text="@LuCustomerFlightNumber"/>
    <action action="ch.minova.afis.LuSupplierAircraftCreditCardCompanyForm" id="LuSupplierAircraftCreditCardCompany" text="@LuSupplierAircraftCreditCardCompany"/>
    <action action="ch.minova.afis.LuSupplierItemPriceForm" icon="LuSupplierItemPreis.ico" id="LuSupplierItemPrice" text="@LuSupplierItemPrice"/>
    <action action="ch.minova.afis.LuSupplierCustomerForm" icon="LuSupplierCustomer.ico" id="LuSupplierCustomer" text="@LuSupplierCustomer"/>
    <action action="ch.minova.afis.LuCustomerItemPriceForm" id="LuCustomerItemPrice" text="@LuCustomerItemPrice"/>
    <!-- Abschluss -->
    <action action="ch.minova.afis.closing.DailyClosingForm" icon="Closing.ico" id="DailyClosing" text="@DailyClosing"/>
    <action action="ch.minova.job.JobResultForm" icon="DataExchange.ico" id="JobResult" text="@JobResult"/>
    <action action="ch.minova.afis.closing.MonthlyClosingForm" icon="ClosingMonthly.ico" id="MonthlyClosing" text="@MonthlyClosing"/>
    <!-- Information -->
    <action action="ch.minova.afis.FlightScheduleLogForm" id="FlightScheduleLog" text="@FlightScheduleLog"/>
    <action action="ch.minova.statistic.StatisticForm" icon="Statistic.ico" id="Statistic" text="@Statistic"/>
    <!-- ANDERE - Autostart -->
    <action action="ch.minova.system.Log4JMessageForm" id="Log4J" text="Logging" autostart="true" visible="false" dialog="true"/>
    <menu id="main">
        <menu id="masterdata" text="@Masterdata">
            <entry id="TaxResponsable" type="action"/>
            <entry id="Supplier" type="action"/>
            <entry id="Tank" type="action"/>
            <entry id="Item" type="action"/>
            <entry id="CompulsaryItem" type="action"/>
            <entry id="Customer" type="action"/>
            <entry id="TaxCode" type="action"/>
            <entry type="separator"/>
            <entry id="Truck" type="action"/>
            <entry id="Driver" type="action"/>
            <entry type="separator"/>
            <entry id="Aircraft" type="action"/>
        </menu>
        <menu id="handling" text="@Handling">
            <entry id="Dispatch" type="action"/>
            <entry type="separator"/>
            <menu id="deliveries" text="@Deliveries">
                <entry id="PreDeliveries" type="action"/>
                <entry id="Shipment" type="action"/>
                <entry id="RBFuelling" type="action"/>
                <entry type="separator"/>
                <entry id="GAFuelling" type="action"/>
            </menu>
            <entry id="CopyFlightSchedule" type="action"/>
        </menu>
        <menu id="lookup" text="@Lookup">
            <entry id="LuCustomerFlightNumber" type="action"/>
            <entry type="separator"/>
            <entry id="LuSupplierCustomer" type="action"/>
            <entry id="LuSupplierAircraftCreditCardCompany" type="action"/>
            <entry id="LuSupplierItemPrice" type="action"/>
            <entry id="LuCustomerItemPrice" type="action"/>
        </menu>
        <menu id="file" text="@AFIS.TTA">
            <entry id="STIM" type="action"/>
            <entry id="Stock" type="action"/>
            <entry type="separator"/>
            <entry id="ShortEntry" type="action"/>
            <entry id="ShortStockExit" type="action"/>
            <entry id="Internal" type="action"/>
            <entry type="separator"/>
            <entry id="TankMeasurement" type="action"/>
        </menu>
        <menu id="configuration" text="@Configuration">
            <entry id="FlightFilterExchange" type="action"/>
            <entry id="FlightFilter" type="action"/>
            <entry type="separator"/>
            <entry id="FlightFieldPosition" type="action"/>
            <entry id="AircraftType" type="action"/>
            <entry id="Airport" type="action"/>
            <entry id="TruckPool" type="action"/>
            <entry type="separator"/>
            <entry id="CreditCardCompany" type="action"/>
            <entry id="Vat" type="action"/>
            <entry type="separator"/>
            <entry id="CompulsaryName" type="action"/>
            <entry id="CompulsaryStockDetail" type="action"/>
        </menu>
        <menu id="closing" text="@Closing">
            <menu id="day" text="@Day">
                <entry id="DailyClosing" type="action"/>
                <entry id="JobResult" type="action"/>
            </menu>
            <menu id="month" text="@Month">
                <entry id="MonthlyClosing" type="action"/>
            </menu>
        </menu>
        <menu id="information" text="@Information">
            <entry id="FlightScheduleLog" type="action"/>
            <entry id="Statistic" type="action"/>
            <entry id="Log4J" type="action"/>
        </menu>
    </menu>
    <toolbar flat="false">
        <entry id="Supplier" type="action"/>
        <entry id="Tank" type="action"/>
        <entry id="Item" type="action"/>
        <entry id="Customer" type="action"/>
        <entry id="Truck" type="action"/>
        <entry id="Driver" type="action"/>
        <entry id="Aircraft" type="action"/>
        <entry type="separator"/>
        <entry id="FlightFilter" type="action"/>
        <entry id="RBFuelling" type="action"/>
        <entry id="PreDeliveries" type="action"/>
        <entry id="GAFuelling" type="action"/>
        <entry id="Shipment" type="action"/>
        <entry type="separator"/>
        <entry id="DailyClosing" type="action"/>
        <entry id="JobResult" type="action"/>
        <entry id="Dispatch" type="action"/>
    </toolbar>
</main>
